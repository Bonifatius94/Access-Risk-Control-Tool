<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import com.jfoenix.controls.JFXComboBox?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="ui.main.sapqueries.modal.results.views.AnalysisGraphsController" spacing="20">
    <BarChart fx:id="barChart" HBox.hgrow="ALWAYS">
        <xAxis>
            <CategoryAxis fx:id="chartX"/>
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="chartY" />
        </yAxis>
    </BarChart>
    <VBox minWidth="150">
        <VBox alignment="CENTER_LEFT" spacing="5" minWidth="100">
            <Label text="%xAxis"/>
            <JFXComboBox fx:id="chartDataChooser"/>
        </VBox>
    </VBox>
</HBox>
